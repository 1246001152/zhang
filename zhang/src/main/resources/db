CREATE TABLE `OS_GCLOUD_FILE` (
  `FILE_ID` RAW(16) NOT NULL COMMENT '文件ID',
  `FILE_SRC` NVARCHAR2(510) DEFAULT NULL COMMENT '文件存储路径',
  `CONTENT_TYPE` NVARCHAR2(126) DEFAULT NULL COMMENT '文件内容类型',
  `FILE_EXT` NVARCHAR2(14) DEFAULT NULL COMMENT '文件后缀',
  `FILE_SIZE` NUMBER(20) DEFAULT NULL COMMENT '文件大小',
  `HASH_MD5` RAW(16) DEFAULT NULL COMMENT '文件HASH MD5值',
  `HASH_SHA256` RAW(32) DEFAULT NULL COMMENT '文件HASH SHA256值',
  `FILE_WIDTH` INT(11) DEFAULT NULL COMMENT '图片/视频宽度',
  `FILE_HEIGHT` INT(11) DEFAULT NULL COMMENT '图片/视频高度',
  `FILE_TIME` INT(11) DEFAULT NULL COMMENT '音频/视频时长',
  `FILE_LOCATION` NVARCHAR2(510) DEFAULT NULL COMMENT '图片/视频/音频拍摄位置',
  `GEO_LONGITUDE` NUMBER(10,7) DEFAULT NULL COMMENT '经度',
  `GEO_LATITUDE` NUMBER(10,7) DEFAULT NULL COMMENT '纬度',
  `GEO_ACCURACY` NUMBER(11) DEFAULT NULL COMMENT '精度',
  `GEO_ALTITUDE` NUMBER(20) DEFAULT NULL COMMENT '海拔高度',
  `GEO_ALTITUDE_ACCRACY` NUMBER(11) DEFAULT NULL COMMENT '海拔高度精度',
  `CREATE_DATE` DATETIME DEFAULT NULL COMMENT '创建时间',
  PRIMARY KEY (`FILE_ID`)
) ENGINE=INNODB DEFAULT CHARSET=UTF8 COMMENT='GCLOUD FILE '